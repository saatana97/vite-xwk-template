        "vite-svg-loader": "^4.0.0",
        "vue-tsc": "^1.0.12"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "npm run lint"
        ],
        "*.{vue,ts,tsx,js,jsx,css,less,json,md,html}": [
            "npm run prettier"
        ]
    }
}
