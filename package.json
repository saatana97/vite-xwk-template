        "prepare": "husky install"
    },
    "dependencies": {
        "axios": "^1.2.2",
        "element-plus": "^2.2.28",
        "pinia": "^2.0.28",
        "qs": "^6.11.0",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.1",
        "@commitlint/config-conventional": "^17.4.0",
        "@saatana97/eslint-config": "^1.0.0",
        "@types/node": "^18.11.12",
        "@types/qs": "^6.9.7",
        "@vitejs/plugin-legacy": "^3.0.1",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitejs/plugin-vue-jsx": "^3.0.0",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.22.0",
        "husky": "^8.0.3",
        "less": "^4.1.3",
        "lint-staged": "^13.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "typescript": "~4.7.4",
        "unplugin-auto-import": "^0.12.1",
